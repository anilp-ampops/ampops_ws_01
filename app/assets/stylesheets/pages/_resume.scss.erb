body {
  background-color: white;
}

.overview-container {
  padding: 35px;
  background-color: white;
}

.overview-section {
  height: 285px;
}

.overview-left {
  height: 285px;
  background-color: white;
  h1 {
      margin-top: 3px;
      letter-spacing: 2px;
      line-height: 22px;
      font-size: 36px;
      font-weight: 700;
  }
  p {
    font-weight: 400;
    font-size: 14px;
    line-height: 17px;
    width: 430px;
  }
}

.overview-right {
  height: 285px;
  background-color: blue;
}

.aplocation {
  font-weight: 400;
  color: #BDBDBD;
  font-size: 18px;
  line-height: 22px;
  letter-spacing: 2px;
}

.oseperator-box {
  padding-top: 12px;
  padding-bottom: 21px;
}

.oseperator-left {
  border: 1px solid #BDBDBD;
  height: 0px;
  width: 290px;
}

.aolinks-box {
  padding-right: 20px;
  padding-top: 20px;
}

.aolink-btn {
  border: 1px solid #BDBDBD;
  border-radius: 6px;
  background-color: rgba(0,0,0,0.25);
  width: 175px;
  a {
    color: #F2F2F2;
    vertical-align: super;
    position: relative;
  }
}

.fa-github:before {
    margin-left: -73px;
}

